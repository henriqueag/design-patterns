{
    "Serilog": {
        "OutputTemplate": "[{Timestamp:HH:mm:ss.fff} {Level:u3}] [{RequestId}] [{SourceContext}] {Message:lj}{NewLine}{Exception}",
        "MinimumLevel": {
            "Default": "Verbose",
            "Override": {
                "Microsoft": "Error",
                "Microsoft.Hosting": "Information",
                "System": "Error"
            }
        }
    }
}